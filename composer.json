{
    "name": "popphp/pop-nav",
    "description": "Pop Nav Component for Pop PHP Framework",
    "keywords": [
        "nav",
        "navigation",
        "html",
        "acl",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "popphp/pop-acl": "^4.0.0",
        "popphp/pop-dom": "^4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Nav\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Nav\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
