{
    "name": "popphp/pop-nav",
    "description": "Pop Nav Component for Pop PHP Framework",
    "keywords": [
        "nav",
        "navigation",
        "html",
        "acl",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "https://github.com/popphp/pop-nav",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "https://www.popphp.org/"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "popphp/pop-acl": "^4.1.2",
        "popphp/pop-dom": "^4.0.5",
        "popphp/pop-utils": "^2.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Nav\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Nav\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1.x-dev"
        }
    }
}
