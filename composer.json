{
    "name": "popphp/pop-nav",
    "description": "Pop Nav Component for Pop PHP Framework",
    "keywords": [
        "nav",
        "navigation",
        "html",
        "acl",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "New BSD",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "popphp/pop-acl": "2.*@dev",
        "popphp/pop-dom": "2.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Nav\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Nav\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
